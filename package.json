{
  "name": "jet-theme",
  "displayName": "Jet theme",
  "description": "Close to Jetbrains color schema",
  "version": "0.0.8",
  "publisher": "TheQazaqApe",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheQazaqApe/jet-theme.git"
  },
  "icon": "./images/icon.png",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Jet dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Jet dark-color-theme.json"
      },
      {
        "label": "Jet light",
        "uiTheme": "vs",
        "path": "./themes/Jet light-color-theme.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/injection.json",
        "scopeName": "todo-comment.injection",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.rs",
          "source.go"
        ]
      }
    ]
  }
}
