{
  "name": "jet-theme",
  "displayName": "Jet theme",
  "description": "Unofficial close to Jetbrains' color schema",
  "version": "0.0.12",
  "publisher": "TheQazaqApe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheQazaqApe/jet-theme.git"
  },
  "icon": "./images/icon.png",
  "keywords": [
    "dark",
    "dark modern",
    "light",
    "light modern",
    "jet",
    "minimalistic",
    "semantic"
  ],
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Jet dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Jet dark-color-theme.json"
      },
      {
        "label": "Jet light",
        "uiTheme": "vs",
        "path": "./themes/Jet light-color-theme.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/injection.json",
        "scopeName": "todo-comment.injection",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.rust",
          "source.go"
        ]
      }
    ]
  }
}
